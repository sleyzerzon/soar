# proposed by generic operator proposal in propose-mission-operator

sp {elaborate*operator*go-to-location*desired
   (state <s> ^operator <op>
              ^waypoints.waypoint <w>)
   (<w> ^id <id>)
   (<op> ^name go-to-location
         ^id <id>)
-->
   (<op> ^desired-waypoint <w>)
}

   
sp {apply*go-to-location*complete
   (state <s> ^operator <op>
              ^top-state.current-waypoint <w>
              ^mission.current <c>)
   (<op> ^name go-to-location
         ^desired-waypoint <w>)
-->
   (<c> ^status complete)
}


sp {speech-monitor*waypoints*destination-waypoint-goto
   (state <s> ^operator <o>
              ^io.output-link <out>)
   (<o> ^name go-to-location
        ^desired-waypoint.id <id>)
-->
   (<out> ^send-message <sm>)
   (<sm> ^destination say
         ^first <first>)
   (<first> ^word going
            ^next <second>)
   (<second> ^word to
             ^next <third>)
   (<third> ^word <id>
            ^next nil)
}
