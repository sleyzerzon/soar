sp {UrCsimple*propose*do-nothing
   (state <s> ^name UrCsimple
              ^last-stage wait)
-->
   (<s> ^operator <op> + =)
   (<op> ^name do-nothing)
}

sp {apply*do-nothing
   (state <s> ^operator <op>
              ^io.output-link <out>)
   (<op> ^name do-nothing)
-->
   (<s> ^last-stage wait -
        ^last-stage do-nothing)
   (<out> ^do <info>)
   (<info> ^info nothing)
}