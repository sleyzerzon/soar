sp {propose*fire-on-command
   (state <s> ^io <io>
             -^text-input-number <num>)
   (<io> ^input-link.text <text>
         ^output-link <ol>)
   (<text> ^next <next>
           ^text-input-number <num>)
   (<next> ^value fire)
   -(<ol> ^text-output.sentence.text-input-number <num>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name fire-on-command
        ^actions.fire.weapon missile
        ^text-input-number <num>)
   
}

sp {apply*fire-on-command
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name fire-on-command
         ^text-input-number <num>)
-->
   (<s> ^text-input-number <num>)
   (<ol> ^text <to>)
   (<to> ^next <next>
         ^text-input-number <num>)
   (<next> ^value missile
          ^next <nw1>)
   (<nw1> ^value fired
          ^next nil)
}
