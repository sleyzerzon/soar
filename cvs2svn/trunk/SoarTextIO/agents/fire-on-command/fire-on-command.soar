sp {propose*fire-on-command
   (state <s> ^io <io>
             -^sentence-num <num>)
   (<io> ^input-link.text-input.sentence <sent>
         ^output-link <ol>)
   (<sent> ^word fire
           ^sentence-num <num>)
   -(<ol> ^text-output.sentence.sentence-num <num>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name fire-on-command
        ^actions.fire.weapon missile
        ^sentence-num <num>)
   
}

sp {apply*fire-on-command
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name fire-on-command
         ^sentence-num <num>)
-->
   (<s> ^sentence-num <num>)
   (<ol> ^text-output <to>)
   (<to> ^sentence <sen>)
   (<sen> ^sentence-num <num>
          ^word missile
          ^next-word <nw1>)
   (<nw1> ^word fired)
}
