sp {propose*I-Dont-Know
   (state <s> ^io <io>
             -^sentence-num <num>)
   (<io> ^input-link.text-input.sentence <sent>
         ^output-link <ol>)
   (<sent> ^word <fw>
           ^sentence-num <num>)
  -(<ol> ^text-output.sentence.sentence-num <num>)
-->
   (<s> ^operator <op> + <)
   (<op> ^name I-Dont-Know
         ^sentence-num <num>)
}



sp {apply*I-Dont-Know
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name I-Dont-Know
         ^sentence-num <num>)
-->
   (<s> ^sentence-num <num>)
   (<ol> ^text-output <to>)
   (<to> ^sentence <sen>)
   (<sen> ^sentence-num <num>
          ^word I
          ^next-word <nw1>)
   (<nw1> ^word cannot
          ^next-word <nw2>)
   (<nw2> ^word do
          ^next-word <nw3>)
   (<nw3> ^word that)
}
