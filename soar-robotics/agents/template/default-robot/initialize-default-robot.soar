sp {propose*initialize-default-robot
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-default-robot)
}

sp {apply*initialize-default-robot
   (state <s> ^operator <op>)
   (<op> ^name initialize-default-robot)
-->
   (<s> ^name default-robot)
}

