sp {propose*initialize
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> + >)
   (<o> ^name initialize)
}

sp {apply*initialize
   (state <s> ^operator <op>)
   (<op> ^name initialize)
-->
   (<s> ^name simple-robot
        ^parameters <p>)
   (<p> ^linear-velocity 0.5
        ^angular-velocity 10
        ^range-tolerance 1.0)
}

