sp {process-message*propose*ignore-unknown-message
   (state <s> ^name process-message)
-->
   (<s> ^operator <op> +)
   (<op> ^name ignore-unknown-message)
}

sp {apply*ignore-unknown-message
   (state <s> ^operator <op>
              ^superstate.operator.message.id <id>
              ^io.output-link <ol>)
   (<op> ^name ignore-unknown-message)
-->
   (<ol> ^remove-message.id <id>)
   (write (crlf) |Ignoring unknown message|)
   (exec player-print |Ignoring unknown message|)
}
