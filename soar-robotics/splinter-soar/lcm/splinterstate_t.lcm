struct splinterstate_t
{
	int64_t utime;	// timestamp
	double baselinemeters;
	double tickmeters;
	double widthmeters;
	double heightmeters;

	int32_t leftodom; 		// odometry
	int32_t rightodom; 		// odometry
	double xyoffset[2];		// constant to subtract from coords so that initial pose is 0,0
	pose_t pose;			// pose
}
