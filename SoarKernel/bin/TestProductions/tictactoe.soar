sp {propose*move
	(state <s> ^io.input-link <il>)
	(<il> ^square <sp>)
	(<sp> ^content EMPTY)
-->
	(<s> ^operator <o> + =)
	(<o> ^name move
		 ^space <sp>)
}

sp {apply*move
	(state <s> ^operator <o>
			   ^io.output-link <ol>)
	(<o> ^name move
	     ^space.pos <pos>)
-->
	(<ol> ^move.pos <pos>)
}


sp {remove*complete*commands
	(state <s> ^operator <o>
	           ^io.output-link <ol>)
    (<ol> ^<attr> <val>)
	(<val> ^status complete)
-->
	(<ol> ^<attr> <val> -)
}
