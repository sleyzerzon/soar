sp {init*adjacent-appear
	(state <s> ^elaborations.adjacent)
-->
	(<s> ^adjacent-appear yes)
}

sp {elaborate*_clear__object__side_
   (state <s> ^gs <g> ^facts <f> ^elaborations <e> ^adjacent-appear yes)
   (<g> ^object <o>)
   (<o> ^p1 <object> ^p2 <type>)
   (<f> ^side <s1>)
   (<s1> ^p1 <side>)
   -{
     (<e> ^adjacent <a>)
     (<a> ^p1 <object> ^p2 <anyobject> ^p3 <side>)
   }
-->
   (<e> ^clear <clear>)
   (<clear> ^p1 <object> ^p2 <side>)
}


sp {elaborate*_unsupported__object_
{
  (state <s> ^gs <g> ^elaborations <e> ^adjacent-appear yes)
  (<o> ^p1 <object> ^p2 { <type> <> ground})
  (<g> ^object <o>)
  (<s> ^name game)
  -{
    (<a> ^p1 <object> ^p2 <anyobject>)
    (<e> ^adjacent <a>)
    (<a> ^p3 bottom)
  }
}
-->
   (<u> ^p1 <object> )
   (<e> ^unsupported <u> )
}