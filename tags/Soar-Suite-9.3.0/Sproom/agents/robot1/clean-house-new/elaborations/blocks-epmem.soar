sp {clean-house*elaborate*block
   (state <s> ^name robot
              ^parameters.objects-held-in epmem
              ^current-location <area>
             -^current-location.storage-area yes
              ^io.input-link.objects.object <obj>)
   (<s> ^blocks <b>)
   (<obj> ^x <x> ^y <y>
          ^id <id>
          ^type <type>)
-->
   (<b> ^block <nb>)
   (<nb> ^x <x> ^y <y>
         ^id <id>
         ^in-storage-room no
         ^area <area>
         ^class block
         ^type <type>)
}
sp {clean-house*elaborate*block*storage
   (state <s> ^name robot
              ^current-location.storage-area yes
              ^current-location <area>
              ^parameters.objects-held-in epmem
              ^io.input-link.objects.object <obj>)
   (<s> ^blocks <b>)
   (<obj> ^x <x> ^y <y>
          ^id <id>
          ^type <type>)
-->
   (<b> ^block <nb>)
   (<nb> ^x <x> ^y <y>
         ^id <id>
         ^area <area>
         ^in-storage-room yes
         ^class block
         ^type <type>)
}
