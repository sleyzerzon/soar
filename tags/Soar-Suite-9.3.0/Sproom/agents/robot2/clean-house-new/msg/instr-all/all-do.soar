sp {instr-all*propose*all-do
   (state <s> ^name instr-all)
-->
   (<s> ^operator <op> + =)
   (<op> ^name all-do)
}

sp {apply*all-do*ied
   (state <s> ^operator <op>
              ^superstate.operator.label <l>
              ^top-state.blocks.block <b>)
   (<op> ^name all-do)
  -(<b> ^<l> true)
-->
   (<b> ^<l> true)
}

sp {apply*all-do*done
   (state <s> ^operator <op>
              ^superstate.operator <ss-op>)
   (<op> ^name all-do)
-->
   (<ss-op> ^done true)
}

