sp {clean-house-new*propose*record-label
   (state <s> ^name robot
              ^blocks.block <b>
              ^labels.label <l>)
   (<b> ^id <id>
        ^<attr> <value>
       -^<label> true)
   (<l> ^attr <attr>
        ^value <value>
        ^label <label>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name record-label
         ^block <b>
         ^label <label>)
}

sp {clean-house-new*propose*record-label*compare
   (state <s> ^name robot
              ^operator <op1> +
              ^operator <op2> +)
   (<op1> ^name record-label)
   (<op2> ^name {<> record-label <> msg})
-->
   (<s> ^operator <op1> > <op2>)
} 

sp {clean-house-new*propose*record-label*multiple
   (state <s> ^name robot
              ^operator <op1> +
              ^operator {<> <op1> <op2>} +)
   (<op1> ^name record-label)
   (<op2> ^name record-label)
-->
   (<s> ^operator <op1> = <op2>)
}  

sp {apply*record-label
   (state <s> ^operator <op>)
   (<op> ^name record-label
         ^block <b>
         ^label <label>)
-->
   (<b> ^<label> true)
}


