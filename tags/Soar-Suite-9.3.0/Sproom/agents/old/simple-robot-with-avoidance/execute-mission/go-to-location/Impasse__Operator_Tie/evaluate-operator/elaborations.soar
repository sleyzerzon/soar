sp {evaluate-operator*elaborate*substate*desired
   (state <s> ^name go-to-location
              ^superstate.operator <so>
              ^superstate.superstate.desired-waypoint <d>)
   (<so> ^name evaluate-operator)
-->
   (<s> ^desired-waypoint <d>)}

sp {go-to-location*elaborate*success
   (state <s> ^name go-to-location
              ^superstate <ss>
              ^desired <d>
              ^top-state.current-waypoint.id <id>)
   (<d> ^id <id>)
   (<ss> ^operator <so>
         ^evaluation <e>)
   (<so> ^name evaluate-operator
         ^evaluation <e>
         ^evaluation.desired <d>)
-->
   (<e> ^symbolic-value success)}