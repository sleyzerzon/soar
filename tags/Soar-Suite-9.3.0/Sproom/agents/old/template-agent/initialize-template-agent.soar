sp {propose*initialize-template-agent
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-template-agent)
}

sp {apply*initialize-template-agent
   (state <s> ^operator <op>)
   (<op> ^name initialize-template-agent)
-->
   (<s> ^name template-agent)
}

