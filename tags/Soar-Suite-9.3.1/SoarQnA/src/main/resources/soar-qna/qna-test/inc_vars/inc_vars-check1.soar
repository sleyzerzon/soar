sp {inc_vars*propose*inc_vars-check1
   (state <s> ^name inc_vars
              ^result <r>
             -^superstate.io.output-link.qna-next)
-->
   (<s> ^operator <op> + =)
   (<op> ^name inc_vars-check1)
}

sp {apply*inc_vars-check1*success
   (state <s> ^operator <op>
              ^superstate.io <io>)
   (<op> ^name inc_vars-check1)
   (<io> ^output-link <out>)
   (<out> ^qna-query <q>)
   (<q> ^status complete
        ^id <q-id>
        ^result <r>)
   (<r> ^features.key 10
        ^next pending)
-->
   (<out> ^qna-next.query <q-id>)
}
