sp {explore-attack*prefer*already-attacking
   (state <s> ^operator <o1> +
              ^operator <o2> +
              ^name explore-attack
              ^task-id <tid>
              ^io.output-link.command <c>
              ^io.input-link.groups.group <g>)
   (<c> ^task-id <tid>
        ^name attack
        ^group1 <target>)
   (<g> ^id <rejected>
       -^type barracks)
   (<o1> ^name orts-attack
         ^enemy-group <target>)
   (<o2> ^name orts-attack
        -^enemy-group <target>)
   (<o2> ^enemy-group <rejected>)
-->
   (<s> ^operator <o1> > <o2>)
}


