sp {explore-attack*elaborate*dist-to-enemy-base
   (state <s> ^name base-defend
              ^top-state.explore-attack <ea>
              ^io.input-link.groups.group <g>
              ^task-id <tid>
              ^my-id <me>)
   (<ea> ^task-id <tid>
         ^enemy-base <eb>)
   (<eb> ^x <bx>
         ^y <by>)
   (<g> ^owner <me>
        ^x-pos <x>
        ^y-pos <y>
        ^id <id>)
-->
   (<s> ^enemy-base <e>)
   (<e> ^x <x>
        ^y <y>
        ^dist (+ (* (- <bx> <x>) (- <bx> <x>))
                 (* (- <by> <y>) (- <by> <y>))))
}

sp {explore-attack*propose*forget-enemy-cc
   (state <s> ^name explore-attack
              ^io.input-link.groups <gs>
              ^top-state.explore-attack <ea>
              ^task-id <tid>
              ^enemy-base <eb2>)
# we don't see a cc  
-{(<gs> ^group <g>)
   (<g> ^enemy 1
        ^type controlCenter
        ^x-pos <x>
        ^y-pos <y>)}
   (<ea> ^task-id <tid>)
   # but are close to where it should be
  (<ea> ^enemy-base <eb>)
    (<eb> ^x <x>
          ^y <y>)
    (<eb2> ^x <x>
           ^y <y>
           ^dist < 100)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name ea*forget-enemy-cc
        ^x <x>
        ^y <y>)
}

sp {explore-attack*apply*forget-enemy-cc
   (state <s> ^name explore-attack
