sp {explore-attack*propose*remember-enemy-cc
   (state <s> ^name explore-attack
              ^io.input-link.groups.group <g>
              ^top-state.explore-attack <ea>
              ^task-id <tid>)
   (<g> ^enemy 1
        ^type controlCenter
        ^x-pos <x>
        ^y-pos <y>)
   (<ea> ^task-id <tid>)
  -{(<ea> ^enemy-base <eb>)
    (<eb> ^x <x>
          ^y <y>)}
-->
   (<s> ^operator <o> + > =)
   (<o> ^name ea*remember-enemy-cc
        ^x <x>
        ^y <y>)
}

sp {explore-attack*apply*remember-enemy-cc
   (state <s> ^name explore-attack
              ^operator <o>
