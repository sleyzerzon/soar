sp {elaborate*waypoints*clear
   (state <s> ^name jon-agent
              ^waypoints <w>)
   (<w> -^waypoint)
-->
   (<s> ^waypoints <w> -)
}

sp {elaborate*waypoints*1
   (state <s> ^name jon-agent
             -^io.input-link.self.waypoints.waypoint.id |1|)
-->
   (<s> ^waypoints.waypoint <w>)
   (<w> ^id |1| ^x 0. ^y 0.)
}

