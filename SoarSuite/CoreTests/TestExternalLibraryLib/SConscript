#!/usr/bin/python
# Project: Soar <http://soar.googlecode.com>
# Author: Jonathan Voigt <voigtjr@gmail.com>
#

Import('compEnv')
env = compEnv.Clone()

if env['STATIC_LINKED']:
	print "Skipping TestExternalLibraryLib due to static linkage"
else:
	TestExternalLibraryLib = env.SharedLibrary('TestExternalLibraryLib', Glob('*.cpp'))
	Install('$PREFIX/lib', TestExternalLibraryLib)

