sp {robot*elaborate*state
   (state <s> ^name robot
              ^superstate nil)
-->
   (<s> ^parameters <p>)
   (<p> ^pickup-range 1.0    # range in meters
        ^low-linear-velocity .1
        ^medium-linear-velocity .4
        ^high-linear-velocity .5
        ^low-angular-velocity 5
        ^medium-angular-velocity 40
        ^high-angular-velocity 40
        ^waypoint-tolerance 0.3 # how close (in meters) the agent needs to be to a waypoint to be considered "at" it
        ^min-progress-distance 0.1 # how much progress the agent has to make when trying to get to a location for it to be considered progress (i.e., to reset cutoff time)
        ^message-update-time 3 # after how many seconds (as reported on the input-link) the agent should send another positition update of an unfriendly entity)
        ^rotation-tolerance 8 # how many degrees the agent's yaw can be off when it has a desired yaw
        ^range-tolerance 1 # distance at which a range arc is considered blocked
        ^progress-update-time 20 # how often progress is updated
        ^max-angular-distance 5
        ^min-angular-distance -5
                              
        ^areas-held-in wm
        ^objects-held-in wm
                         
        ^look-ahead-planning yes
        ^search-control-go-to-gateway yes)
}