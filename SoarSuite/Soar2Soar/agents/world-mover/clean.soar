sp {world-mover*propose*clean
   (state <s> ^name world-mover
              ^io.output-link.<cmd>.status)
-->
   (<s> ^operator <op> + =)
   (<op> ^name clean)
}


sp {apply*move*clean
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name clean)
   (<ol> ^<cmd-name> <cmd>)
   (<cmd> ^status)
-->
   (<ol> ^<cmd-name> <cmd> -)
}
