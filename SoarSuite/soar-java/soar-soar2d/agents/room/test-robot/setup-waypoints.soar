sp {test-robot*propose*setup-waypoints
   (state <s> ^name test-robot
             -^waypoints)
-->
   (<s> ^operator <op> + >)
   (<op> ^name setup-waypoints)
}

sp {apply*setup-waypoints
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name setup-waypoints)
-->
   (<s> ^waypoints done)
   (<ol> ^add-waypoint <w1>
         ^add-waypoint <w2>
         ^add-waypoint <w3>
         ^add-waypoint <w4>)
   (<w1> ^id 1 ^x 5 ^y 5)
   (<w2> ^id 2 ^x 6 ^y 5)
   (<w3> ^id 3 ^x 6 ^y 6)
   (<w4> ^id 4 ^x 5 ^y 6)
}
