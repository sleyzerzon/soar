sp {orient*propose*rotate*right
   (state <s> ^name orient
              ^yaw <yaw> > 0
              ^io.input-link.self.angle.direction <d>)
-->
   (<s> ^operator <op> +)
   (<op> ^name rotate-right
         ^actions.rotate.direction right)
}

sp {apply*rotate-right
   (state <s> ^operator <op>
             -^rotated-once)
   (<op> ^name rotate-right)
-->
   (<s> ^rotated-once true)
}

