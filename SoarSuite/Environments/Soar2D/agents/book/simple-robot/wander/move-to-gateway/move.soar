sp {move-to-gateway*propose*move
   (state <s> ^name move-to-gateway
              -^need-to-turn true
              -^moving true)
-->
   (<s> ^operator <op> +)
   (<op> ^name move
         ^actions.move.direction forward)
}

sp {move-to-gateway*propose*stop
   (state <s> ^name move-to-gateway
              ^need-to-turn true
              ^moving true)
-->
   (<s> ^operator <op> +)
   (<op> ^name move
         ^actions.move.direction stop)
}
