sp {features*propose*features-init
   (state <s> ^name features
             -^feature)
-->
   (<s> ^operator <op> + =)
   (<op> ^name features-init)
}

sp {apply*features-init
   (state <s> ^operator <op>)
   (<op> ^name features-init)
-->
   (<s> ^feature nil)
}

sp {apply*features-init*todo
   (state <s> ^operator <op>
              ^superstate.io.output-link.agents.agent <a>)
   (<op> ^name features-init)
-->
   (<s> ^todo <a>)
}
