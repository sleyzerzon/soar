sp {blocks-world*propose*success
   (state <s> ^name blocks-world
              ^io.input-link.goal achieved)
-->
   (<s> ^operator <op> + !)
   (<op> ^name success)
}

sp {blocks-world*compare
   (state <s> ^operator <op1> + {<> <op1> <op2>} +)
   (<op1> ^name success)
-->
   (<s> ^operator <op1> > <op2>)
}   

sp {apply*success
   (state <s> ^operator <op>)
   (<op> ^name success)
-->
   (halt)
}
