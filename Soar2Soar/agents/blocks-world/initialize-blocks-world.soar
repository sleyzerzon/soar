sp {propose*initialize-blocks-world
   (state <s> ^superstate nil
              ^soar2soar ready
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-blocks-world)
}

sp {apply*initialize-blocks-world
   (state <s> ^operator <op>)
   (<op> ^name initialize-blocks-world)
-->
   (<s> ^name blocks-world)
}

