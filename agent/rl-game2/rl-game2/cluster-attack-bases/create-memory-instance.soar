sp {cluster-attack-bases*propose*create-memory-instance
   (state <s> ^name cluster-attack-bases
              ^instance <si>
              ^top-state.cluster-attack-bases-memory <m>)
   (<m> -^instance.label <si>)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-memory-instance)}

sp {cluster-attack-bases*apply*create-memory-instance
   (state <s> ^name cluster-attack-bases
              ^instance <si>
              ^top-state.cluster-attack-bases-memory <m>
              ^operator <o>)
   (<o> ^name create-memory-instance)
-->
   (<m> ^instance <mi>)
   (<mi> ^label <si>)}