sp {monitor*mission
   (state <s> ^superstate nil
              ^mission.current.id <nid>)
-->
   (write (crlf) |Going to | <nid>)
}

sp {monitor*waypoint
   (state <s> ^superstate nil
             -^mission.current.id <wid>
              ^current-waypoint.id <wid>)
-->
   (write (crlf) |      via | <wid>)
}