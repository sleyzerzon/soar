sp {patrol*propose*goto-next-area
   (state <s> ^name patrol
             -^achieved-area true
              ^next-area.id <id>
              ^top-state.areas.area <area>)
   (<area> ^id <id>)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name goto-next-area
         ^type go-to-area
         ^area <area>)
}

sp {patrol*propose*goto-next-area*next-to
   (state <s> ^name patrol
             -^achieved-area true
              ^next-area.id <id>
              ^top-state.current-location. << neighbor-area next >> <area>)
   (<area> ^id <id>)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name goto-next-area
         ^type go-to-area
         ^area <area>)
}
