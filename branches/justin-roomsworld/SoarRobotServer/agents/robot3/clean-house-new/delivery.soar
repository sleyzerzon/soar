sp {retrieve-task*interrupt
   (state <s> ^name retrieve-task)
-->
   (interrupt)
}

sp {retrieve-task*propose*initialize
   (state <s> ^name retrieve-task
             -^to-be-retrieved
             -^retrieved)
-->
   (<s> ^operator <op> + = >)
}

sp {retrieve-task*apply*initialize
   (state <s> ^name retrieve-task
              ^superstate.operator.task <task>)
-->
   (<s> ^to-be-retrieved.lti <task>
        ^retrieved <retrieved>)
}

sp {retrieve-task*propose*retrieve-lti
   (state <s> ^name retrieve-task
              ^to-be-retrieved.lti <lti>)
-->
   (<s> ^operator <op> + = >)
   (<op> ^name retrieve-lti
         ^lti <lti>)
}

sp {retrieve-task*apply*retrieve-lti*retrieve
   (state <s> ^name retrieve-task
              ^operator <op>
              ^top-state.parameters.tasks-held-in smem
              ^smem.command <scmd>
             -^retrieved.lti <lti>)
   (<op> ^name retrieve-lti
         ^lti <lti>)
-->
   (<scmd> ^retrieve <lti>)
}

sp {retrieve-task*apply*retrieve-lti*copy-children
   (state <s> ^name retrieve-task
              ^operator <op>
              ^top-state.parameters.tasks-held-in smem
              ^smem.result.retrieved <lti>
              ^to-be-retrieved <to-be-retrieved>
              ^retrieved <retrieved>)
   (<op> ^name retrieve-lti
         ^lti <lti>)
   (<lti> ^<attr> <val>)
   (<retrieved> -^lti <val>)
-->
   (<to-be-retrieved> ^lti <val>)
}

sp {retrieve-task*apply*retrieve-lti*remove-command
   (state <s> ^name retrieve-task
              ^operator <op>
              ^top-state.parameters.tasks-held-in smem
              ^smem <smem>
              ^to-be-retrieved <to-be-retrieved>
              ^retrieved <retrieved>)
   (<op> ^name retrieve-lti
         ^lti <lti>)
   (<smem> ^command <scmd>
           ^result.<< success failure bad-cmd >>)
   (<scmd> ^retrieve <lti>)
   (<to-be-retrieved> ^lti <lti>)
-->
   (<scmd> ^retrieve <lti> -)
   (<to-be-retrieved> ^lti <lti> -)
   (<retrieved> ^lti <lti>)
}

sp {retrieve-task*propose*done
   (state <s> ^name retrieve-task
             -^to-be-retrieved.lti
              ^retrieved.lti)
-->
   (<s> ^operator <op> + = >)
   (<op> ^name done)
}

sp {retrieve-task*apply*done
   (state <s> ^name retrieve-task
              ^operator.name done
              ^superstate.operator.task <task>)
-->
   (<task> ^all-retrieved yes)
}

# pick up

sp {delivery*propose*pickup-block
   (state <s> ^name delivery
              ^io.input-link.objects.object <obj>
              ^tasks.task <task>)
   (<task> ^object <desc>)
   (<obj> ^id <id>)
   # check that the object is right for the task
  -{ (<desc> ^<attr> <val>)
    -(<obj> ^<attr> <val>) }
-->
   (<s> ^operator <op> + = >)
   (<op> ^name pickup-block
         ^task <task>
         ^id <id>)
}

sp {interrupt-pickup-block
   (state <s> ^operator.name grasp-block)
-->
   (interrupt)
}

sp {delivery*apply*pickup-block*store-object-id
   (state <s> ^name delivery
              ^operator <op>)
   (<op> ^name pickup-block
         ^task <task>
         ^id <id>)
   (<task> -^object-id)
-->
   (<task> ^object-id <id>)
}

# put down

sp {delivery*propose*move-to-room-center
   (state <s> ^name delivery
              ^io.input-link.self.carry.object.id <id>
              ^top-state.current-location <room>
              ^tasks.task <task>)
   (<task> ^object-id <id>
           ^destination <dest>)
   # check that we are in the right location
  -{ (<dest> ^<attr> <val>)
    -(<room> ^<attr> <val>) }
-->
   (<s> ^operator <op> + = >)
   (<op> ^name move-to-room-center
         ^object <obj>)
}

sp {interrupt-drop-block
   (state <s> ^superstate.name delivery
              ^name move-to-room-center
              ^operator.name drop-block)
-->
   (interrupt)
}

sp {move-to-room-center*apply*drop-block*complete-task
   (state <s> ^name move-to-room-center
              ^operator <op>
              ^superstate <ss>)
   (<op> ^name drop-block
         ^actions.drop-object.id <id>)
   (<ss> ^name delivery
         ^operator.object <obj>
         ^tasks <tasks>)
   (<tasks> ^task <task>)
   (<task> ^object <obj>)
   (<obj> ^id <id>)
-->
   (<tasks> ^task <task> -)
}

