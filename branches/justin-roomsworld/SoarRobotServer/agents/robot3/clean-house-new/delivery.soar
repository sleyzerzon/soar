# retrieve-task

sp {delivery*propose*retrieve-task*ecological-doors
   (state <s> ^name delivery
              ^top-state <ts>
              ^tasks.task <task>)
   (<ts> ^parameters <params>
         ^current-location.type door)
   (<params> ^tasks-held-in smem
             ^method-ecological-doors yes)
   (<task> -^all-retrieved)
-->
   (<s> ^operator <op> + = >)
   (<op> ^name retrieve-task
         ^task <task>)
}

