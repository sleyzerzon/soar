sp {features*propose*features-done
   (state <s> ^name features
              ^feature nil
             -^agent
             -^todo)
-->
   (<s> ^operator <op> + =)
   (<op> ^name features-done)
}

sp {apply*features-done
   (state <s> ^operator <op>
              ^superstate <ss>)
   (<op> ^name features-done)
-->
   (<ss> ^features ready)
}
