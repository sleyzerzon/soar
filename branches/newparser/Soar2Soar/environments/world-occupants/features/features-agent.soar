sp {features*propose*features-agent
   (state <s> ^name features
              ^feature nil
             -^agent
              ^todo <a>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name features-agent
         ^agent <a>)
}

sp {apply*features-agent
   (state <s> ^operator <op>
              ^feature nil
              ^features <f>)
   (<op> ^name features-agent
         ^agent <a>)
-->
   (<s> ^agent <a>
        ^feature nil - <f>)
}
