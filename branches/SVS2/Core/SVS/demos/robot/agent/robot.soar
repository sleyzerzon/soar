source extract.soar
source models.soar
source control.soar
source generate.soar

sp {init
   (state <s> ^superstate nil)
-->
   (<s> ^time 0 ^auto-control t)}

sp {propose*count-time
   (state <s> ^superstate nil ^time <t>)
-->
   (<s> ^operator <o> +)
   (<o> ^name count-time)}

sp {apply*count-time
   (state <s> ^superstate nil
              ^time <t>
              ^operator.name count-time)
-->
   (<s> ^time <t> - (+ <t> 1))}
