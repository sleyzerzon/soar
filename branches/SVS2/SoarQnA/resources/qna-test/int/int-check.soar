sp {int*propose*int-check
   (state <s> ^name int
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name int-check)
}

sp {apply*int-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name int-check)
-->
   (write |int test failed!|)
   (halt)
}

sp {apply*int-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query int)
   (<op> ^name int-check)
-->
   (<t> ^done true)
   (write |int: success|)
}
