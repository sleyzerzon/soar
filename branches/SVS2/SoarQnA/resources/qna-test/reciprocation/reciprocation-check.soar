sp {reciprocation*propose*reciprocation-check
   (state <s> ^name reciprocation
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name reciprocation-check)
}

sp {apply*reciprocation-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name reciprocation-check)
-->
   (write |reciprocation test failed!|)
   (halt)
}

sp {apply*reciprocation-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query |/|
        ^num two)
   (<op> ^name reciprocation-check)
-->
   (<t> ^done true)
   (write |reciprocation: success|)
}
