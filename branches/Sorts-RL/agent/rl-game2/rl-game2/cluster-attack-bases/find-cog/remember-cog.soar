sp {find-cog*propose*remember-cog
   (state <s> ^name find-cog
              ^grouping-radius-exploded t
              ^superstate.instance <si>
              ^top-state.cluster-attack-bases-memory.instance <mi>
              ^my-pid <mpid>
              ^io.input-link <in>)
   (<mi> ^instance <si>
        -^cog)
   (<in> ^vision-state.grouping-radius 1000000
         ^groups.group <single-tank-group>)
   (<single-tank-group> ^owner <mpid>
                        ^type tank
                        ^x-pos <x>
                        ^y-pos <y>)
-->
   (<s> ^operator <o> +)
   (<o> ^name remember-cog
        ^cog-x <x>
        ^cog-y <y>)}

sp {find-cog*apply*remember-cog
   (state <s> ^name find-cog
              ^superstate.instance <si>
              ^top-state <ts>
              ^operator <o>)
   (<ts> ^cluster-attack-bases-memory.instance <mi>)
   (<mi> ^label <si>)
   (<o> ^name remember-cog
        ^cog-x <x>
        ^cog-y <y>)
-->
   (<mi> ^cog <cog>)
   (<cog> ^x <x>
          ^y <y>)}