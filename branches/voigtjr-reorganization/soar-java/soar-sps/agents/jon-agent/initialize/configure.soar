sp {initialize*propose*configure
   (state <s> ^name initialize
              ^top-state.configure <c>)
   (<c> ^<att> <val>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name configure
         ^configure.<att> <val>)
}

sp {initialize*apply*configure
   (state <s> ^name initialize
              ^operator <op>
              ^top-state.io.output-link <ol>)
   (<op> ^name configure
         ^configure.<att> <val>)
-->
   (<ol> ^configure <c>)
   (<c> ^<att> <val>)
}

sp {initialize*compare*configure
   (state <s> ^name initialize
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name configure)
   (<op2> ^name add-waypoint)
-->
   (<s> ^operator <op1> > <op2>)
}

