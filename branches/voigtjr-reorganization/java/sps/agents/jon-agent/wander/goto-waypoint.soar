sp {wander*propose*goto-waypoint
   (state <s> ^name wander
              ^top-state.io.input-link.self.waypoints.waypoint <wp>)
   (<wp> ^target true)
-->
   (<s> ^operator <op> +)
   (<op> ^name goto-waypoint)
}

sp {wander*compare*goto-waypoint
   (state <s> ^name wander
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name goto-waypoint)
   (<op2> ^name choose-waypoint)
-->
   (<s> ^operator <op1> > <op2>)
}
