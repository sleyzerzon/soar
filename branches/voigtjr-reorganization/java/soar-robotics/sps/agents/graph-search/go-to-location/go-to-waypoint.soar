sp {go-to-location*propose*go-to-waypoint
   (state <s> ^name go-to-location
              ^top-state.current-waypoint.next <next-wp>)
-->
   (<s> ^operator <op> + )
   (<op> ^name go-to-waypoint
         ^next-waypoint <next-wp>)
}
sp {apply*go-to-waypoint
   (state <s> ^name go-to-location
              ^operator <op>
              ^top-state <ts>)
   (<ts> ^current-waypoint <c-wp>)
   (<op> ^name go-to-waypoint
         ^next-waypoint <next-wp>)
-->
   (<ts> ^current-waypoint <c-wp> -
                           <next-wp>)
}
