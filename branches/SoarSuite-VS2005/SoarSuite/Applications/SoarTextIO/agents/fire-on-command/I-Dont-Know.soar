sp {propose*I-Dont-Know
   (state <s> ^io <io>
             -^text-input-number <num>)
   (<io> ^input-link.text <text>
         ^output-link <ol>)
   (<text> ^text-input-number <num>
           ^next <sent>)
   (<sent> ^value <fw>)
  -(<ol> ^text-output.sentence.text-input-number <num>)
-->
   (<s> ^operator <op> + <)
   (<op> ^name I-Dont-Know
         ^text-input-number <num>)
}



sp {apply*I-Dont-Know
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name I-Dont-Know
         ^text-input-number <num>)
-->
   (<s> ^text-input-number <num>)
   (<ol> ^text <to>)
   (<to> ^next <sen>
         ^text-input-number <num>)
   (<sen> ^value I
          ^next <nw1>)
   (<nw1> ^value cannot
          ^next <nw2>)
   (<nw2> ^value do
          ^next <nw3>)
   (<nw3> ^value that
          ^next nil)
}
