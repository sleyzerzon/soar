 ##########################################
 # None of the following rules should be marked
 # as duplicates when sourced by Soar.
 ##########################################
 
 sp {other-rule
     (state <s> ^operator <o1> + ^operator <o2> +)
     (<o1> ^name joe)
     (<o2> ^name fred)
     -->
    (<s> ^operator <o1> >)}


    
 sp {other-rule3
     (state <s> ^operator <o1> + ^operator <o2> +)
     (<o1> ^name joe)
     (<o2> ^name fred)
     -->
    (<s> ^operator <o1> >)
    (<s> ^operator <o2> = 1)}
    
    
sp {RL-1
 (state <s> ^operator <o1> + ^operator <o2> +)
 (<o1> ^name joe)
 (<o2> ^name fred)
 -->
 (<s> ^operator <o1> = 0)}
 
sp {temp
  :template
    (state <s> ^operator <o1> + ^operator <o2> +)
    (<o1> ^name joe)
    (<o2> ^name fred)
    -->
 (<s> ^operator <o1> = 0)}
 
  
 sp {RL-3
  (state <s> ^operator <o1> + ^operator <o2> +)
  (<o1> ^name joe)
  (<o2> ^name fred)
  -->
 (<s> ^operator <o2> = 0)}
 
 

 
 sp {temp4
   :template
     (state <s> ^operator <o1> + ^operator <o2> +)
     (<o1> ^name joe)
     (<o2> ^name fred)
     -->
 (<s> ^operator <o2> = 0)}
 
