sp {retreat*propose*no-op
   (state <s> ^name retreat
              ^io.input-link.clock <clock>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name no-op
        ^actions.no.action <clock>)
}

sp {apply*remove-no-op
   (state <s> ^operator <op>
              ^io <io>)
   (<io> ^input-link.clock <clock>
         ^output-link <ol>)
   (<ol> ^no <no>)
   (<no> ^action <> clock)
   
-->
   (<ol> ^no <no> -)
}
