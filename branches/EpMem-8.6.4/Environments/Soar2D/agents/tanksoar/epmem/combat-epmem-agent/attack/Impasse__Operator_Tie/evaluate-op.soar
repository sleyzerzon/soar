#IF there is an operator that caused the tie impasse
#AND it does not have a recalled status
#THEN propose evaluating it
sp {epmem-evals*propose*evaluate-op
   (state <s> ^name epmem-evals
              ^item <item>
              ^top-state.evaluations <evals>)
   (<item> ^name { <name>  << move turn move-and-fire turn-and-fire fire-missile >> }
           ^direction <dir>)
  -{(<evals> ^eval <eval>)
    (<eval> ^name <name>
            ^direction <dir>
            ^value <val>)}
-->
   (<s> ^operator <op> + =)
   (<op> ^name evaluate-op
         ^eval-item <item>
         ^eval-name <name>
         ^eval-direction <dir>)
}
