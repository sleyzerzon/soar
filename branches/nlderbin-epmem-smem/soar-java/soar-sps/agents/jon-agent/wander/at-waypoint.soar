sp {wander*propose*at-waypoint
   (state <s> ^name wander
              ^at <id>
              ^top-state.io.input-link.self.waypoints.waypoint <wp>)
   (<wp> ^id <id>
         ^target true)
-->
   (<s> ^operator <op> + >)
   (<op> ^name at-waypoint
         ^waypoint <wp>)
}

sp {wander*apply*at-waypoint
   (state <s> ^name wander
              ^operator.name at-waypoint
              ^operator.waypoint <wp>)
   (<wp> ^target true)
-->
   (<wp> ^target true -)
}
