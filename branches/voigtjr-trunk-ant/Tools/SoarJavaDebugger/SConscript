#!/usr/bin/python
import glob
import SoarSCons

Import('env')
env = env.Clone()

theJar = '#Tools/SoarJavaDebugger/SoarJavaDebugger.jar';
sources = SoarSCons.JavaSources('src')

Depends(theJar, 'build.xml')
env.Command(theJar, sources, 'ant', chdir=1)
env.Clean(theJar, '#Tools/SoarJavaDebugger/build')

