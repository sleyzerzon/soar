echo "\nLoading attack/remove-manuevers.soar"


### propose op to remove old manuever data
### - each manuever has a time stamp
sp {all*propose*remove*manuever*completed
   (state <s> ^status.manuever <man>
              ^io.input-link.clock <ck>)
   (<man> ^time < <ck>)
-->
   (<s> ^operator <o> + =, >)
   (<o> ^name remove-manuever
        ^manuever <man>)
#   (write (crlf) |Proposing remove-manuever|)
}

## delete old manuever
sp {all*apply*remove*manuever
   (state <s> ^operator <o>
              ^status <st>)
   (<o> ^name remove-manuever
        ^manuever <man>)
-->
   (<st> ^manuever <man> -)
#   (write (crlf) |Removing old manuever|)
}

