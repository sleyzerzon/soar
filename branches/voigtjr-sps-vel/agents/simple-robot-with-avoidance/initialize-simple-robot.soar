sp {propose*initialize-simple-robot
   (state <s> ^superstate nil
             -^name simple-robot)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-simple-robot)
}

sp {apply*initialize-simple-robot
   (state <s> ^operator <op>)
   (<op> ^name initialize-simple-robot)
-->
   (<s> ^name simple-robot
        ^entities <e>)
}

sp {apply*initialize-simple-robot*config
   (state <s> ^operator <op>
              ^io.output-link <out>)
   (<op> ^name initialize-simple-robot)
-->
   (<out> ^configure.yaw-format float)
}

