sp {test-robot*propose*clear-messages
   (state <s> ^name test-robot
              ^io.input-link.self.name <me>
              ^io.input-link.self.received-messages.message <m>)
   (<m> ^from <me>
        ^id <id>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name remove-message
         ^id <id>)
}

