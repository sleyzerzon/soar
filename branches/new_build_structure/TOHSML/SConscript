import os

Import('env')

install = []
TOHSML = env.Program('TOHSML', Glob('*.cpp'))
install.append(env.Install('$OUT_DIR', TOHSML))
install.append(env.Install('$OUT_DIR/Demos', 'towers-of-hanoi-SML.soar'))
env.Alias('TOHSML', install)
