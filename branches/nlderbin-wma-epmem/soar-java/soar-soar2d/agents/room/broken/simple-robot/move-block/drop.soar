sp {move-block*propose*drop
   (state <s> ^name move-block
              ^io.input-link.self.carry.id <bid>
              ^io.output-link.rotate-relative.status complete)
-->
   (<s> ^operator <op> + =)
   (<op> ^name drop
         ^actions.drop.id <bid>)
}

sp {apply*drop
   (state <s> ^operator <op>
              ^top-state <ts>)
   (<ts> ^block-id <bid>)
   (<op> ^name drop)
-->
   (<ts> ^block-id <bid> -)
}
