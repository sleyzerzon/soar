sp {propose*initialize-test-robot
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-test-robot)
}

sp {apply*initialize-test-robot
   (state <s> ^operator <op>)
   (<op> ^name initialize-test-robot)
-->
   (<s> ^name test-robot)
}
