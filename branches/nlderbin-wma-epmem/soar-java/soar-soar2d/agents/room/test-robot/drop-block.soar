sp {test-robot*propose*drop-block
   (state <s> ^name test-robot
              ^io.input-link.self.carry.id <id>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name drop-block
         ^id <id>)
}

sp {apply*drop-block
   (state <s> ^operator <op>
              ^io.output-link <ol>)
   (<op> ^name drop-block
         ^id <id>)
-->
   (<s> ^had-carried <id>)
   (<ol> ^drop-object.id <id>)
#         ^set-velocity <sv>)
#   (<sv> ^linear-velocity 1.
#         ^angular-velocity 0.)
}
