sp {go-to-location*elaborate*substate*desired
   (state <s> ^name go-to-location
              ^superstate.operator <so>)
   (<so> ^name go-to-location
         ^desired-waypoint <d>)
-->
   (<s> ^desired-waypoint <d>)
}

sp {go-to-location*elaborate*problem-space
   (state <s> ^name go-to-location)
-->
   (<s> ^problem-space <p>)
   (<p> ^name go-to-location
        ^default-state-copy yes
        ^dont-copy near-waypoint
        ^two-level-attributes top-state)}

sp {go-to-location*evaluate*state*success
   (state <s> ^desired <de>
              ^desired-waypoint <d>
              ^problem-space.name go-to-location
              ^top-state.current-waypoint <d>)
-->
   (<s> ^success <de>)}

sp {go-to-location*elaborate*next-location
   (state <s> ^name go-to-location
              ^top-state.waypoints.waypoint <wp>
              ^superstate.operator <so>)
   (<so> ^name go-to-location
         ^desired-waypoint.id <id>)
   (<wp> ^id <id>
         ^x <x>
         ^y <y>)
-->
   (<s> ^desired <wp>)
}