sp {goto-waypoint*propose*turn-to
   (state <s> ^name goto-waypoint
              ^target-yaw <yaw>)
-->
   (<s> ^operator <op> +)
   (<op> ^name turn-to
         ^yaw <yaw>)
}

sp {goto-waypoint*apply*turn-to
   (state <s> ^name goto-waypoint
              ^operator <op>
              ^top-state.io.output-link <ol>)
   (<op> ^name turn-to
         ^yaw <yaw>)
-->
   (<ol> ^set-heading.yaw <yaw>)
}

sp {goto-waypoint*compare*turn-to
   (state <s> ^name goto-waypoint
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name turn-to)
   (<op2> ^name drive)
-->
   (<s> ^operator <op1> > <op2>)
}

