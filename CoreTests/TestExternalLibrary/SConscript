#!/usr/bin/python
# Project: Soar <http://soar.googlecode.com>
# Author: Jonathan Voigt <voigtjr@gmail.com>
#
Import('compEnv')
if compEnv['STATIC_LINKED']:
	print "Skipping TestExternalLibrary due to static linkage"
else:
	TestExternalLibrary = compEnv.Program('TestExternalLibrary', Glob('*.cpp'))
	compEnv.Install('$PREFIX/bin', TestExternalLibrary)

