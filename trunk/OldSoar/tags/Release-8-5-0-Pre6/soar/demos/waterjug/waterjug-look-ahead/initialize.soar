sp {waterjug*propose*initialize
   (state <s> ^name waterjug
             -^jug)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize)}

sp {waterjug*apply*initialize
   (state <s> ^operator.name initialize)
-->
   (<s> ^jug <i> <j>
        ^desired.jug <k>)
   (<i> ^volume 3
        ^contents 0)
   (<j> ^volume 5
        ^contents 0)
   (<k> ^volume 3
        ^contents 1)}
