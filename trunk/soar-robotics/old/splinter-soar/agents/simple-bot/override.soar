sp {simple-bot*propose*override
   (state <s> ^name simple-bot
              ^io.input-link.override <over>)
   (<over>    ^active true
              ^motor.left <left>
              ^motor.right <right>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name override
         ^motor <move>)
   (<move> ^left <left>
           ^right <right>)
}

sp {simple-bot*apply*override
   (state <s> ^operator <o>
              ^io.output-link <ol>)
   (<o> ^motor <move>)
   (<move> ^left <left>
           ^right <right>)
-->
   (<ol> ^motor <move>)
   (<move> ^left <left>
          ^right <right>)
}
