sp {simple-robot*propose*execute-mission
   (state <s> ^name simple-robot
              ^mission <m>)
-->
   (<s> ^operator <op> +)
   (<op> ^name execute-mission
         ^mission <m>)
}

sp {apply*execute-mission*init-current
   (state <s> ^operator <op>)
   (<op> ^name execute-mission
         ^mission <m>)
   (<m> ^first <f>)
-->
   (<m> ^current <f>)
}

sp {apply*execute-mission*next
   (state <s> ^operator <op>
              ^mission <m>)
   (<op> ^name execute-mission)
   (<m> ^current <c>)
   (<c> ^status complete
        ^next <n>)
-->
   (<m> ^current <c> - <n>)
}