pushd simple-robot-with-mission
source simple-robot-with-mission_source.soar
popd

sp {apply*initialize-simple-robot*create-mission
   (state <s> ^operator <op>)
   (<op> ^name initialize-simple-robot)
-->
   (<s> ^mission <m>)
   (<m> ^first <s1>)
   (<s1> ^name start
         ^delay-time 0
         ^next <s2>)
   (<s2> ^name go-to-location
         ^x -6 ^y 1
         ^next <s3>)
   (<s3> ^name scout
         ^next nil)
}