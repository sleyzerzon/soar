sp {wander*propose*go-to-waypoint
   (state <s> ^name wander
              ^superoperator.destination <w>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name go-to-waypoint
         ^destination <w>)
}

sp {apply*go-to-waypoint
   (state <s> ^operator <op>)
   (<op> ^name go-to-waypoint
         ^destination <w>)
   (<w> ^x <x>
        ^y <y>)
-->
   (<s> ^io.output-link.move-to <mt>)
   (<mt> ^x <x>
         ^y <y>
         ^throttle 1.0)
}