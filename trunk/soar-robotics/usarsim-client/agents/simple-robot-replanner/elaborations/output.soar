sp {apply*operator*create-action-command*zero-parameters
   (state <s> ^operator <o>
              ^io.output-link <ol>)
   (<o> ^actions <act>)
   (<act> ^<att> <value>)
  -(<value> ^<att2> <value2>) #no attributes
-->
   (<ol> ^<att> <value3>)
}

sp {apply*operator*create-action-command*one-parameter
   (state <s> ^operator <o>
              ^io.output-link <ol>)
   (<o> ^actions <act>)
   (<act> ^<att> <value>)
   (<value> ^<att2> <value2>)
  -(<value> ^{<attX> <> <att2>}) #no other attributes besides the first one
-->
   (<ol> ^<att> <value3>)
   (<value3> ^<att2> <value2>)
}

# currently second param is hard coded as thottle to prevent multiple matches from multiple orderings
sp {apply*operator*create-action-command*two-parameters
   (state <s> ^operator <o>
              ^io.output-link <ol>)
   (<o> ^actions <act>)
   (<act> ^<att> <value>)
   (<value> ^<att21> <value21>
            ^{<att22> <> <att21> throttle} <value22>)
-->
   (<ol> ^<att> <value3>)
   (<value3> ^<att21> <value21>
             ^<att22> <value22>)}

sp {apply*operator*remove-command
   (state <s> ^operator
              ^io.output-link <ol>)
   (<ol> ^<att> <value>)
   (<value> ^status complete)
-->
   (<ol> ^<att> <value> -)}
