sp {propose*initialize-rl-tank
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-rl-tank)
}

sp {apply*initialize-rl-tank
   (state <s> ^operator <op>
              ^io.output-link <out>)
   (<op> ^name initialize-rl-tank)
-->
   (<s> ^name rl-tank)
   (<out> ^sorts <sorts>)
   (cmd |source rl-rules.soar|)}

