sp {apply*operator*remove-command*complete
   (state <s> ^operator
              ^io.output-link <ol>)
   (<ol> ^<att> <value>)
   (<value> ^status complete)
-->
   (<ol> ^<att> <value> -)
}

sp {apply*operator*remove-command*interrupted
   (state <s> ^operator
              ^io.output-link <ol>)
   (<ol> ^<att> <value>)
   (<value> ^status interrupted)
-->
   (<ol> ^<att> <value> -)
}
