sp {apply*cdl*add-used-goodthing
   (state <s> ^name game
              ^operator.name check-for-duplicate-location
              ^using-goodthing {<> -1 <ug>}) # -1 is special, don't use it up
-->
   (<s> ^used-goodthing <ug>)
}

sp {elaborate*inherit*used-goodthing
   (state <s> ^name game
              ^duplicate-of <s2>)
   (<s2> ^used-goodthing <ug>)
-->
   (<s> ^used-goodthing <ug>)
}
