sp {elaborate*ts*goodthings
   (state <s> ^superstate nil)
-->
   (<s> ^good-things <gt>)
}

# test running script adds productions to add actual goodthings

# p1..p3 Appearences

sp {apply*usi*goodthing
   (state <s> ^name game
              ^operator.name update-search-info
              ^gs <gs>
              ^old-gs <old-gs>
              ^current-evaluation-depth <ced>
              ^top-state.good-things.indicator <gt>)
   (<gt> ^index <idx>
         ^type addition
         ^what <w>)
   (<w> ^<item> <id-gt>)
   (<id-gt> ^p1 <p1-val>
           -^p2)
   (<gs> ^<item> <id-gs>)
   (<id-gs> ^p1 <p1-val>)
  -{(<old-gs> ^<item> <id-oldgs>)
    (<id-oldgs> ^p1 <p1-val>)}
                   -{(<s> ^used-goodthing <idx>)}
-->
   (<s> ^current-evaluation-depth <ced> -
                                  (+ <ced> 1))
   (<s> ^using-goodthing <idx>)
   (write (crlf) |Bump due to appearence of | <item> | | <p1-val>)
}


