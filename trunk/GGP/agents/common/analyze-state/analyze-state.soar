   (state <s> ^name game
              ^superstate nil
              ^operator.name analyze-state
              ^gs <gs>
              ^old-gs <old-gs>)
   (<gs> ^<item> <id-gs>)
   (<id-gs> ^p1 <p1-val>
            ^p2 <p2-val>
           -^p3)
  -{(<old-gs> ^<item> <id-oldgs>)
    (<id-oldgs> ^p1 <p1-val>
                ^p2 <p2-val>)}
                   -{(<f> ^bookkeeping-state <item>)}
-->
   (write (crlf) |ADDED | <item> | ^p1 | <p1-val> | ^p2 | <p2-val>)
}

