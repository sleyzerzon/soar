sp {elaborate*ts*goodthings
   (state <s> ^superstate nil)
-->
   (<s> ^good-things <gt>)
   (<gt> ^holding.p1 log1)
   (<gt> ^holding.p1 log2)
   (<gt> ^holding.p1 hammer1)
   (<gt> ^holding.p1 combo1)
   (<gt> ^compromised.p1 water1)
}

