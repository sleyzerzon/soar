sp {apply*game-op*remove-old-copy
   (state <s> ^operator.name {<> update-state <> analyze-state <> update-search-info
          <> save-location <> check-for-duplicate-location <> record-terminal-state <name>}
              ^gs <gs>
              ^old-gs <ogs>)
