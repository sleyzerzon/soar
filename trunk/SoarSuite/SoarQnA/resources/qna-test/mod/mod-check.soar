sp {mod*propose*mod-check
   (state <s> ^name mod
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name mod-check)
}

sp {apply*mod-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name mod-check)
-->
   (write |mod test failed!|)
   (halt)
}

sp {apply*mod-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query mod)
   (<op> ^name mod-check)
-->
   (<t> ^done true)
   (write |mod: success|)
}
