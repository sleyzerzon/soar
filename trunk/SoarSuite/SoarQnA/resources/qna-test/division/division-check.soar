sp {division*propose*division-check
   (state <s> ^name division
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name division-check)
}

sp {apply*division-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name division-check)
-->
   (write |division test failed!|)
   (halt)
}

sp {apply*division-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query |/|
        ^num one)
   (<op> ^name division-check)
-->
   (<t> ^done true)
   (write |division: success|)
}
