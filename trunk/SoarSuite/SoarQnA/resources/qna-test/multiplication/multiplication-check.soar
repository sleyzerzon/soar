sp {multiplication*propose*multiplication-check
   (state <s> ^name multiplication
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name multiplication-check)
}

sp {apply*multiplication-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name multiplication-check)
-->
   (write |multiplication test failed!|)
   (halt)
}

sp {apply*multiplication-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query |*|)
   (<op> ^name multiplication-check)
-->
   (<t> ^done true)
   (write |multiplication: success|)
}
