sp {subtraction*propose*subtraction-check
   (state <s> ^name subtraction
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name subtraction-check)
}

sp {apply*subtraction-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name subtraction-check)
-->
   (write |subtraction test failed!|)
   (halt)
}

sp {apply*subtraction-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query |-|
        ^num one)
   (<op> ^name subtraction-check)
-->
   (<t> ^done true)
   (write |subtraction: success|)
}
