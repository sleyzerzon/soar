sp {negation*propose*negation-check
   (state <s> ^name negation
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name negation-check)
}

sp {apply*negation-check*failure
   (state <s> ^operator <op>
              ^result <r>
              ^superstate.io.input-link.qna-resultset.result.result {<> <r>})
   (<op> ^name negation-check)
-->
   (write |negation test failed!|)
   (halt)
}

sp {apply*negation-check*success
   (state <s> ^operator <op>
              ^result <r>
              ^superstate <ss>)
   (<ss> ^io.input-link.qna-resultset.result.result <r>
         ^test <t>)
   (<t> ^source math
        ^query |-|
        ^num two)
   (<op> ^name negation-check)
-->
   (<t> ^done true)
   (write |negation: success|)
}
