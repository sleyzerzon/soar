sp {inc_vars*propose*inc_vars-check2
   (state <s> ^name inc_vars
              ^result <r>
              ^cursor)
-->
   (<s> ^operator <op> + =)
   (<op> ^name inc_vars-check2)
}

sp {apply*inc_vars-check2
   (state <s> ^operator <op>
              ^superstate <ss>
              ^cursor <c>)
   (<ss> ^test <t>
         ^io.input-link.qna-resultset <rs>)
   (<t> ^source test_db
        ^query all_vars
        ^num two)
   (<rs> ^cursor <c>
         ^result <r>)
   (<r> ^key 9
        ^next {<> nil})
   (<op> ^name inc_vars-check2)
-->
   (<t> ^done true)
   (write |inc_vars: success|)
}
