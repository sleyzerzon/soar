sp {wander*propose*drop
   (state <s> ^name move-to-gateway)
   (<s> ^top-state <ts>)
   (<ts> ^io.input-link.self <self>
         ^drop-cycle <cycle>)
   (<self> ^cycle <cycle>
           ^carry.id <objid>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name drop
        ^actions.drop.id <objid>)
}

sp {apply*drop
   (state <s> ^operator <op>
              ^top-state <ts>)
   (<op> ^name drop)
-->
   (<ts> ^drop-cycle <cycle> -)
}

