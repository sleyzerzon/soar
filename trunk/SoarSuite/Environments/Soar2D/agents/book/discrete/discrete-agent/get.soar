sp {discrete-agent*propose*get
   (state <s> ^name book
              ^io.input-link <il>)
   (<il> ^object <obj>
        -^self.carry)
   (<obj> ^range < 17
          ^id <id>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name get
         ^actions.get.id <id>)
}

sp {discrete-agent*compare*get
   (state <s> ^name book
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name get)
   (<op2> ^name << move-forward rotate-left rotate-right >> )
-->
   (<s> ^operator <op1> > <op2>)
}
