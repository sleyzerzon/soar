sp {monitor*waypoints*at-waypoint
   (state <s> ^superstate nil
              ^current-waypoint <a>)
   (<a> ^x <x> ^y <y>)
-->
   (write (crlf) |Current-waypoint: (| <x> |,| <y> |)|)
}

sp {monitor*waypoints*at-waypoint*top
   (state <s> ^superstate nil
              ^current-waypoint <a>)
   (<a> ^x <x> ^y <y>)
-->
   (exec player-print |Current-waypoint: (| <x> |,| <y> |)|)
}

