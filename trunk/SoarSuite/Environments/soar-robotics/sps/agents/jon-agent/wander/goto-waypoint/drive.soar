sp {goto-waypoint*propose*drive
   (state <s> ^name goto-waypoint)
-->
   (<s> ^operator <op> +)
   (<op> ^name drive)
}

sp {goto-waypoint*apply*drive
   (state <s> ^name goto-waypoint
              ^operator <op>
              ^top-state.io.output-link <ol>)
   (<op> ^name drive)
-->
   (<ol> ^set-velocity <sv>)
   (<sv> ^linear-velocity 0.5
         ^angular-velocity 0.)
}
