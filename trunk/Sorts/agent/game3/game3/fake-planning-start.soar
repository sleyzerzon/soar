sp {apply*start-task
   (state <s> ^operator <o>
              ^top-state <ts>)
   (<ts> ^planning.execution-buffer <eb>)
   (<o> ^name start-task
        ^task-id <tid>
        ^task-name <tn>)
-->
   (<eb> ^task <t>)
   (<t> ^id <tid>
        ^name <tn>)
}


sp {propose*execute-task
   (state <s> ^name sorts
             -^running-task
              ^planning.execution-buffer <eb>)
   (<eb> ^task <t>)
   (<t> ^name <nm>
        ^id <tid>
       -^completed true)
-->
   (<s> ^operator <o> + =)
   (<o> ^name execute-task
        ^task <t>)
}

sp {apply*execute-task
   (state <s> ^operator <o>
              ^top-state <ts>)
   (<o> ^name execute-task
        ^task <t>)
   (<t> ^name <nm>
        ^id <tid>)
-->
   (<ts> ^running-task <rt>)
   (<rt> ^name <nm>
         ^id <tid>)
}