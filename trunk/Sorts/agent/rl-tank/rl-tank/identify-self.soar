sp {propose*identify-self
   (state <s> ^name rl-tank
              -^my-pid
              ^io.input-link.game-info.player-id <id>)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name identify-self
        ^my-id <id>)}

sp {apply*identify-self
   (state <s> ^operator <o>)
   (<o> ^name identify-self
        ^my-id <id>)
-->
   (<s> ^my-pid <id>)}