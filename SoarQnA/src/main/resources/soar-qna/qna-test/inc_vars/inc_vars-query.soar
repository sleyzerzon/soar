sp {inc_vars*propose*inc_vars-query
   (state <s> ^name inc_vars
             -^result)
-->
   (<s> ^operator <op> + =)
   (<op> ^name inc_vars-query)
}

sp {apply*inc_vars-query
   (state <s> ^operator <op>
              ^superstate.io.output-link <out>)
   (<op> ^name inc_vars-query)
-->
   (<s> ^result foo)
   (<out> ^qna-query <q>)
   (<q> ^source test_db
        ^query all_vars
        ^parameters <ps>
        ^results incremental)
}
