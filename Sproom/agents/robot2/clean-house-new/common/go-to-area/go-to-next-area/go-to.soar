sp {apply*go-to*next-area-gate-way
   (state <s> ^name << goto-not-clean-area go-to-next-area goto-storage-area go-to-seen-block >>
              ^top-state.current-location <ca>
              ^operator <op>)
   (<op> ^name go-to
         ^destination <gw>)
   (<ca> ^neighbor-area <na>)
   (<gw> ^to <id>)
   (<na> ^id <id>)
-->
   (<na> ^previous-area <ca>)
}

sp {apply*go-to*next-area-gate-way*remove
   (state <s> ^name  << goto-not-clean-area  go-to-next-area goto-storage-area go-to-seen-block  >>
              ^top-state.current-location <ca>
              ^operator <op>)
   (<op> ^name go-to
         ^destination <gw>)
   (<op> ^name go-to
         ^destination <gw>)
   (<ca> ^neighbor-area <na>)
   (<gw> ^to <id>)
   (<na> ^id <id>
         ^previous-area { <> <ca> <pa> })
-->
   (<na> ^previous-area <pa> -)
}
