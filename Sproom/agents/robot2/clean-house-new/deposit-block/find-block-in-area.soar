sp {deposit-block*propose*find-block-in-area
   (state <s> ^name deposit-block
              ^current-location <ca>)
   (<ca> ^cleaned no
        -^storage-area yes
         ^type room)
-->
   (write (crlf) |Search area while holding block.|)
   (<s> ^operator <op> + =, >)
   (<op> ^name find-block-in-area)
 }
 
 sp {deposit-block*compare*find-block-in-area
   (state <s> ^name deposit-block
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name find-block-in-area)
   (<op2> ^name goto-storage-area)
-->
   (<s> ^operator <op1> > <op2>)
}
