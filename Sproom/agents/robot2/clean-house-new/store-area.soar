sp {clean-house*propose*store-area
#   :interrupt
   (state <s> ^name robot
              ^parameters.areas-held-in smem
              ^current-location.store yes)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name store-area)
}

sp {apply*store-area
   (state <s> ^operator.name store-area
              ^current-location <ca>
              ^smem.command <cmd>)
-->
   (<cmd> ^store <ca>)
}

sp {apply*store-area*sucess
   (state <s> ^operator.name store-area
              ^current-location <ca>
              ^smem.result.success)
-->
   (<ca> ^store yes -)
}
