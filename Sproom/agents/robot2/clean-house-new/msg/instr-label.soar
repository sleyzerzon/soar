# syntax: label attribute value label
sp {msg*propose*instr-label
   (state <s> ^name msg
              ^message.first <first>)
   (<first> ^word label
            ^next <second>)
   (<second> ^word <attr>
             ^next <third>)
   (<third> ^word <value>
            ^next <fourth>)
   (<fourth> ^word <label>
             ^next nil)
-->
   (<s> ^operator <op> + =)
   (<op> ^name instr-label
         ^attr <attr>
         ^value <value>
         ^label <label>)
}

sp {apply*instr-label
   (state <s> ^operator <op>
              ^superstate.operator <ss-op>)
   (<op> ^name instr-label
         ^done true)
-->
   (<ss-op> ^processed true)
}
