sp {clean-house-new*propose*execute-mission
   (state <s> ^name robot
              ^current-mission <cm>)
   (<cm> -^status << suspended terminated >>)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name execute-mission)
}

sp {apply*execute-mission
   (state <s> ^operator <op>
              ^current-mission <cm>)
   (<cm> ^status initiated)
   (<op> ^name execute-mission)
-->
   (<cm> ^status initiated -
                 executing)
}

sp {clean-house-new*compare*execute-mission
   (state <s> ^name robot
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name execute-mission)
   (<op2> ^name << get-block finished-cleaning >>)
-->
   (<s> ^operator <op1> > <op2>)
}

