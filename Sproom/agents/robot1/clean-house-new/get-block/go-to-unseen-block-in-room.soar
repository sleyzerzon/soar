sp {get-block*propose*go-to-unseen-block-in-room
   (state <s> ^name get-block
              ^current-location <area>
              ^top-state.parameters.pickup-range <r>
              ^top-state.blocks.block <block>)
   (<block> ^area <area>
           -^in-storage-room yes)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name go-to
         ^destination <block>
         ^range <r>)
}
 
 
sp {get-block*compare*pickup*prefer*visible
   (state <s> ^name get-block
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name pickup-block)
   (<op2> ^name go-to)
-->
   (<s> ^operator <op1> > <op2>)
}

